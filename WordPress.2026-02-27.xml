<pre class="wp-block-code"><code lang="php" class="language-php">// ÂÆö‰πâÂ§¥ÂÉèÁºìÂ≠òÁõÆÂΩï
define('AVATAR_CACHE_DIR', WP_CONTENT_DIR . '/avatars');
// ÂÆö‰πâÂ§¥ÂÉèÁºìÂ≠òURL
define('AVATAR_CACHE_URL', WP_CONTENT_URL . '/avatars');
// ÂÆö‰πâÂ§¥ÂÉèÁºìÂ≠òÊúâÊïàÊúüÔºåÈªòËÆ§7Â§©
define('AVATAR_CACHE_LIFETIME', apply_filters('avatar_cache_lifetime', 7 * DAY_IN_SECONDS));
// ÂÆö‰πâÂ§¥ÂÉèÊúÄÂ§ßÂ≠òÂÇ®Êó∂Èó¥ÔºåÈªòËÆ§30Â§©
define('AVATAR_CLEANUP_AGE', apply_filters('avatar_max_age', 30 * DAY_IN_SECONDS));
// ÂÆö‰πâÈªòËÆ§Â§¥ÂÉèURL
define('DEFAULT_AVATAR_URL', '/wp-content/uploads/2025/03/default.webp');

/**
 * ÁºìÂ≠òÁî®Êà∑Â§¥ÂÉèÂà∞Êú¨Âú∞
 *
 * @param string $email Áî®Êà∑ÈÇÆÁÆ±
 * @param array $args Â§¥ÂÉèÂèÇÊï∞
 * @return string Â§¥ÂÉèURL
 */
function cache_avatar_locally($email, $args = array()) {
    // Ê£ÄÊü•WP_FilesystemÂáΩÊï∞ÊòØÂê¶Â≠òÂú®ÔºåËã•‰∏çÂ≠òÂú®ÂàôÂä†ËΩΩÊñá‰ª∂Êìç‰ΩúÁ±ª
    if (!function_exists('WP_Filesystem')) {
        require_once ABSPATH . 'wp-admin/includes/file.php';
    }
    WP_Filesystem();
    global $wp_filesystem;
    // ÈªòËÆ§Â§¥ÂÉèÂèÇÊï∞
    $default_args = array(
        'default' =&gt; DEFAULT_AVATAR_URL,
        'size'    =&gt; 96,
        'rating'  =&gt; 'g',
        'scheme'  =&gt; null
    );
    $args = wp_parse_args($args, $default_args);
    $args = apply_filters('avatar_cache_params', $args, $email);
    // ÂØπÂèÇÊï∞ËøõË°åÊéíÂ∫èÔºå‰ª•Á°Æ‰øùÁîüÊàê‰∏ÄËá¥ÁöÑhashÂÄº
    $sorted_args = ksort_deep($args);
    $param_string = http_build_query($sorted_args);
    // Ê†πÊçÆÁî®Êà∑ÈÇÆÁÆ±ÂíåÂèÇÊï∞ÁîüÊàêÂîØ‰∏ÄÁöÑhashÂÄº
    $hash = md5(strtolower(trim($email)) . '|' . $param_string);
    $base_path = AVATAR_CACHE_DIR . '/' . $hash;
    // Êü•ÊâæÊòØÂê¶Â∑≤ÊúâÁºìÂ≠òÁöÑÂ§¥ÂÉèÊñá‰ª∂
    $avatar_file = find_existing_avatar($base_path);
    if ($avatar_file) {
        // Â¶ÇÊûúÂ§¥ÂÉèÊñá‰ª∂Â≠òÂú®‰∏îÈúÄË¶ÅÂà∑Êñ∞ÔºåÂàôÂÆâÊéíÂà∑Êñ∞Êìç‰Ωú
        if (should_refresh_avatar($avatar_file)) {
            schedule_avatar_refresh($email, $args);
        }
        // ËøîÂõûÂ§¥ÂÉèÁöÑURL
        return str_replace(AVATAR_CACHE_DIR, AVATAR_CACHE_URL, $avatar_file);
    }
    // Ëé∑ÂèñGravatarÂ§¥ÂÉèURL
    $gravatar_url = get_avatar_url($email, $args);
    // Ëé∑ÂèñÂ§¥ÂÉèÂÜÖÂÆπ
    $response = wp_remote_get($gravatar_url, array('timeout' =&gt; 10));
    // Â¶ÇÊûúËØ∑Ê±ÇÂ§±Ë¥•ÔºåÂàôËøîÂõûÈªòËÆ§Â§¥ÂÉèURL
    if (is_wp_error($response) || 200 !== wp_remote_retrieve_response_code($response)) {
        return $args['default'];
    }
    // Ëé∑ÂèñÂõæÁâáÁ±ªÂûã
    $content_type = wp_remote_retrieve_header($response, 'content-type');
    // Ëé∑ÂèñÊñá‰ª∂Êâ©Â±ïÂêç
    $ext = get_extension_from_mime($content_type);
    // ËÆæÁΩÆÁºìÂ≠òÂ§¥ÂÉèÁöÑË∑ØÂæÑ
    $avatar_path = "{$base_path}.{$ext}";
    $avatar_dir = dirname($avatar_path);
    // Â¶ÇÊûúÁºìÂ≠òÁõÆÂΩï‰∏çÂ≠òÂú®ÔºåÂàôÂàõÂª∫ÁõÆÂΩï
    if (!file_exists($avatar_dir)) {
        wp_mkdir_p($avatar_dir);
    }
    // Â∞ÜÂ§¥ÂÉèÂÜÖÂÆπ‰øùÂ≠òÂà∞Êú¨Âú∞
    if ($wp_filesystem-&gt;put_contents($avatar_path, wp_remote_retrieve_body($response), 0644)) {
        return AVATAR_CACHE_URL . "/{$hash}.{$ext}";
    }
    // Â¶ÇÊûú‰øùÂ≠òÂ§±Ë¥•ÔºåÂàôËøîÂõûÈªòËÆ§Â§¥ÂÉèURL
    return $args['default'];
}

/**
 * Ê∑±Â∫¶ÊéíÂ∫èÊï∞ÁªÑ
 *
 * @param array $array ËæìÂÖ•Êï∞ÁªÑ
 * @return array ÊéíÂ∫èÂêéÁöÑÊï∞ÁªÑ
 */
function ksort_deep($array) {
    if (is_array($array)) {
        ksort($array);
        foreach ($array as $key =&gt; $value) {
            $array[$key] = ksort_deep($value);
        }
    }
    return $array;
}

/**
 * Êü•ÊâæÂ∑≤Â≠òÂú®ÁöÑÂ§¥ÂÉèÊñá‰ª∂
 *
 * @param string $base_path Â§¥ÂÉèÁºìÂ≠òÂü∫Á°ÄË∑ØÂæÑ
 * @return string|null Â§¥ÂÉèÊñá‰ª∂Ë∑ØÂæÑÔºåËã•Ê≤°ÊúâÂàôËøîÂõûnull
 */
function find_existing_avatar($base_path) {
    $files = glob("{$base_path}.*");
    foreach ($files as $file) {
        if (is_file($file) &amp;&amp; in_array(pathinfo($file, PATHINFO_EXTENSION), ['jpg', 'jpeg', 'png', 'webp'])) {
            return $file;
        }
    }
    return null;
}</code></pre>
