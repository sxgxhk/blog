             $hd = curl_exec($ch);
             if(!emptyempty($hd) &amp;&amp; !(stripos($hd,‚ÄôContent-Length: image/png‚Äô)||stripos($hd,‚ÄôContent-Length: image/jpg‚Äô))){
                $fp =file_get_contents($pic_url);
